{
  "name": "lingodash",
  "version": "1.0.0",
  "description": "AI-powered vocabulary learning application using Mistral AI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:perf": "vitest run --reporter=verbose src/js/*.perf.test.js",
    "test:perf:watch": "vitest src/js/*.perf.test.js",
    "test:all": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1"
  },
  "devDependencies": {
    "happy-dom": "^15.11.7",
    "vite": "^5.0.0",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  }
}
